<!DOCTYPE html>
<html>
    <head><title>Test User Signup</title></head>
    <body>
        %= form_for "/user/signup" => {format => 'json'} => (method => 'POST') => begin
        Name:
        %= text_field 'signUpName'
        <br/>Email:
        %= email_field 'signUpEmail'
        <br/>Password
        %= password_field 'signUpPassword'
        <br/>
        %= submit_button 'Submit'
    % end
    </body>
</html>
